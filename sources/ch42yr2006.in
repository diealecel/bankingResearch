<!-- itemsortkey:120ACPO -->
<!-- expcite:TITLE 12-BANKS AND BANKING!@!<usCode chapter="42" edition="2006" title="12">CHAPTER 42</usCode>-LOW-INCOME HOUSING PRESERVATION AND RESIDENT HOMEOWNERSHIP!@!SUBCHAPTER I-PREPAYMENT OF MORTGAGES INSURED UNDER NATIONAL HOUSING ACT -->
<!-- field-start:structuralhead -->
<h3 class="subchapter-head">SUBCHAPTER I—PREPAYMENT OF MORTGAGES INSURED UNDER NATIONAL HOUSING ACT</h3>
<!-- field-end:structuralhead -->
<!-- documentid:12_4101  usckey:120000000410100000000000000000000 currentthrough:20070103 documentPDFPage:-1 -->
